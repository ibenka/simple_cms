<%%= render :partial => 'admin/shared/form_errors', :object => @<%= plural_name.singularize %> %>

<%%= simple_form_for @<%= plural_name.singularize %>, :url => url_for(:controller => 'admin/<%= plural_name %>', :action => 'update'), :method => 'put', :html => { :role => "form" } do |form| %>

  <%%= render :partial => 'form', :object => form %>

  <%%= submit_tag 'Submit <%= plural_name.singularize.capitalize %>', :class => 'btn btn-primary' %>

<%% end %>