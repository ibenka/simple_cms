<% for user in @users %>
  <li class="list-group-item">
    <div class="row">
      <div class="col-sm-6">
        <%= link_to user.email, edit_admin_user_path(user) %>
      </div>
      <div class="col-sm-6 text-right">
        <%= link_to(url_for(:controller => 'admin/users', :action => 'destroy', :page_id => user.id), data: { confirm: 'Are you sure?' }) do %>
          <span class="glyphicon glyphicon-trash"></span>
        <% end %>
      </div>
  </li>
<% end %>